{
  "name": "harpoon",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "harpoon.add",
        "title": "Add Harpoon"
      },
      {
        "command": "harpoon.show",
        "title": "Show Harpoons"
      },
      {
        "command": "harpoon.yoink",
        "title": "Yoink a file"
      }
    ],
    "configuration": {
      "title": "Harpoon",
      "properties": {
        "harpoon.keybindings": {
          "type": "array",
          "default": [],
          "description": "Keybindings"
        },
        "harpoon.listkey": {
          "type": ["string", "null"],
          "default": null,
          "description": "Specify what sort of keyevent should be pressed to open the list"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },

  "devDependencies": {
    "@types/node": "^18",
    "@types/vscode": "^1.73.0",
    "typescript": "^5.5.2"
  }
}
